extends layout

block content
    div.index-container
        div.puppy-box
            h1= title
            p Welcome to #{title}
        each imgObj in imgObjs
            div.puppy-box
                picture
                    source(media=`(max-width:630px)` srcset=`${imgObj.sm}`)
                    source(media=`(max-width: 810px)` srcset=`${imgObj.md}`)
                    source(media=`(max-width:900px)` srcset=`${imgObj.cn}`)
                    source(media=`(max-width:1099px)` srcset=`${imgObj.lg}`)
                    source(media=`(min-width:1100px)` srcset=`${imgObj.fs}`) 
                    img.image(src=`${imgObj.fs}` alt=`A dog`)

